<center><h2> Fast Pitch Form </h2></center>
<%= form_for :answers, url: { action: :create } do |f| %>
<% @questions.each do |question| %>
  <%= f.label(question.question_text) %>
  <div><%= f.text_area(question.id) %></div>
<% end %>

<div class="form-group">
  <%= f.submit "Submit", class: "btn btn-success" %>
</div>

<% end %>
